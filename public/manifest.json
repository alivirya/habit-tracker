{
    "manifest_version": 2,

    "name": "Audo",
    "description": "Habit tracker extension",
    "version": "0.0.1",
    "permissions": ["storage", "unlimitedStorage"],

    "browser_action": {
        "default_popup": "index.html",
        "default_title": "Open the popup"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    }
}
